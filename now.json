{
  "version": 2,
  "name": "tmdb",
  "alias": "tmdb.idiot.design",
  "builds": [
    {
      "use": "@now/static-build",
      "src": "package.json",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": "^/favicon.ico", "dest": "/favicon.ico" },
    { "src": "^/(.*)", "dest": "/index.html" }
  ],
  "public": false
}
